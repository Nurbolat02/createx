                label(for="themes1") Sort by 
                .select-wrapper
                  select#themes1(name="category") 
                    option(value="all events") All events
                    option(value="newest") Newest
                    option(value="oldest") Oldest
              .our-events__select 
                label(for="themes2") Show
                .select-wrapper
                  select#themes2(name="category") 
                    option(value="6") 6
                    option(value="9") 9
                    option(value="12") 12
